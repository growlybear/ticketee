<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="utf-8">
  <title>Ticketee</title>
  <%= stylesheet_link_tag "application", :media => "all" %>
  <%= csrf_meta_tags %>
</head>
<body>

  <% flash.each do |key, value| %>
    <div id="<%= key %>" class="flash">
      <%= value %>
    </div>
  <% end %>

  <%= yield %>

  <%= javascript_include_tag "application" %>
</body>
</html>
